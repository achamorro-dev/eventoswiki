---
import { ProvincesLocator } from '@/provinces/di/provinces.locator'
import { ProvinceSelect } from './province-select'

interface Props {
  value?: string
}

const { value } = Astro.props
const provinces = await ProvincesLocator.getProvincesQuery().execute()
---

<ProvinceSelect client:load id={'province-filter'} placeholder="Provincia" value={value} provinces={provinces} />
