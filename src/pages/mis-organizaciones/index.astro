---
import Layout from '@/layouts/layout.astro'
import MyOrganizationsPage from '@/organizations/presentation/client/components/my-organizations-page/my-organizations-page.astro'
import { Urls } from '@/ui/urls/urls'

const userId = Astro.locals.user?.id

if (!userId) {
  return Astro.redirect(Urls.LOGIN)
}
---

<Layout title="Mis organizaciones">
  <MyOrganizationsPage userId={userId} />
</Layout>
